package print01;

public class Print03 {

	public static void main(String[] args) {
		System.out.print("공공데이터 융합 웹 애플리케이션 개발자과정 ");
		System.out.print("강의장 : B강의장");
		System.out.print(" 2/8 ~ 7/20\n");
		System.out.println();
		System.out.println();
		System.out.print("수업중");
		System.out.print(" 입니다");
		

	}

}
