package day02;

public class Char01 {

	public static void main(String[] args) {
		char ch1 = 'A';
		char ch2 = 65;
		char ch3 = '\u0041';
		
		System.out.println(ch1);
		System.out.println(ch2);
		System.out.println(ch3);
		System.out.println("---------------------");	
		char ch4 = 'ê°€';
		char ch5 = 44032;
		
		System.out.println(ch4);
		System.out.println(ch5);
		


	}

}
