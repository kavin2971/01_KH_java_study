package day04;

public class For01 {

	public static void main(String[] args) {
//반복문 : 여러번 반복하여 수행하고자 할때
		
		System.out.println("안녕하세요 KH 정보교육원 입니다");
		System.out.println("안녕하세요 KH 정보교육원 입니다");
		System.out.println("안녕하세요 KH 정보교육원 입니다");
		System.out.println("안녕하세요 KH 정보교육원 입니다");
		System.out.println("안녕하세요 KH 정보교육원 입니다");
		System.out.println("안녕하세요 KH 정보교육원 입니다");
		System.out.println("========================");
	// for (초기식; 조건식; 증감식)
    //     (int a =1; a<10; a+-1씩실행 10까지(9회 반복됨))
		for (int i = 1; i < 5; ++i ) {
			System.out.println("안녕하세요 KH정보교육원 입니다.");
		}
		
	}

}
