package day03;

public class IfElseMulti06 {

	public static void main(String[] args) {
		//
		
		int score = 60;
				
				if (score <=100) { // 100 ~ 마이너스까지
					if (score >=0) {
					
				}else if (score >= 0)
					
						System.out.println("F학점");
					else
						System.out.println("점수는 0점 미만이 될수없습니다.");


	} else 
		System.out.println("점수는 100점 이상이 될수 없습니다.");


 }
}