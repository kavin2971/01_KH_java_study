package variable02;

public class Variable01 {

	public static void main(String[] args) {
		//ì •ìˆ˜
		int number;
		number = 25;
		
		int number2 = 30;
		
		System.out.println(number+number2);
		
		int result = number + number2;
		System.out.println(result);
		System.out.println(result*3);
		System.out.println(result/5);

	}

}
