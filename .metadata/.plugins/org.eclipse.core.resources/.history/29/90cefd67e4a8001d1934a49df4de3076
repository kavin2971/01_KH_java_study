package day03;

public class If01 {

	public static void main(String[] args) {
		//
		
		int score = 80;
		
		if(score>=80) {
			System.out.println("합격입니다");
		}
		if(score<80) 
			System.out.println("재평가 대상입니다");
		
		if(score>=90) {
			System.out.println("합격입니다");
			System.out.println("우수한 성적입니다");
		}

	

  }
	
}
