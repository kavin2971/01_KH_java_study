package operator03;

public class Operator02 {

	public static void main(String[] args) {
		
	       int n1 = -20;
	       n1 = +n1;
	       
	       System.out.println(n1);
	       
	       n1 = -n1;
	       
	       System.out.println(n1);
	       
	       int x = 10;
	       int y = 7;
	       int result = x / y;
	    		   System.out.println(result); //int ÷ int = 몫
	    		   
	    		   double result2 = (double)x/y;
	    		   System.out.println(result2);
}

	}


