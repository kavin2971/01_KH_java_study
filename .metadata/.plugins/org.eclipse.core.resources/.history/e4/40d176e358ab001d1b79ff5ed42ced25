package day04;

import java.util.Scanner;

public class For04 {

	public static void main(String[] args) {
		// 
		
		Scanner sc = new Scanner(System.in);
		System.out.println("구구단 1~9단 사이의 숫자를 입력하세요");
		int num = sc.nextInt();
		for (int i=1; i<=9; i++) {
			System.out.println("5*"+i+"="+5*i);
		}
	 
		}
			

	}


